{
  "name": "mood-blobs",
  "version": "0.1.0",
  "description": "Expressive SVG blob components representing different moods for React",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.esm.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "type-check": "tsc --noEmit",
    "demo": "npx serve website"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.4.0"
  },
  "keywords": ["react", "svg", "blob", "mood", "emotion", "animation", "components", "ui"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/mood-blobs"
  }
}
